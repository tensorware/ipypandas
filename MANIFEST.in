include LICENSE
include README.md

include pyproject.toml
include pytest.ini

include install.json
include package.json
include tsconfig.json
include babel.config.js
include webpack.config.js
include ipypandas/labextension/*.tgz

# Typescript
graft src
prune lib
prune **/node_modules

# Documentation
graft docs
exclude docs/\#*
prune docs/build

# Tests
graft tests
prune tests/build

# Examples
graft examples

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .github
global-exclude .vscode
global-exclude .ipynb_checkpoints
