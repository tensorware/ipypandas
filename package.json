{
    "name": "ipypandas",
    "version": "0.1.0",
    "description": "Interactive JupyterLab features for the python data analysis library pandas.",
    "keywords": [
        "widgets",
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "files": [
        "dist/**/*.js",
        "lib/**/*.js"
    ],
    "homepage": "https://github.com/tensorware/ipypandas",
    "bugs": {
        "url": "https://github.com/tensorware/ipypandas/issues"
    },
    "license": "BSD-3-Clause",
    "author": {
        "name": "Tensorware",
        "email": "development@tensorware.net"
    },
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "style": "./src/styles/index.css",
    "repository": {
        "type": "git",
        "url": "https://github.com/tensorware/ipypandas"
    },
    "scripts": {
        "test": "jest",
        "lint": "eslint . --ext .ts,.tsx",
        "build": "yarn run build:less && yarn run build:lib && yarn run build:labextension",
        "build:lib": "tsc && webpack",
        "build:less": "lessc src/styles/index.less src/styles/index.css",
        "build:labextension": "jupyter labextension build .",
        "clean": "yarn run clean:lib && yarn run clean:labextension",
        "clean:lib": "rimraf .yarn lib dist docs/build",
        "clean:labextension": "rimraf ipypandas/labextension",
        "watch": "npm-run-all -p 'watch:*'",
        "watch:lib": "tsc -w",
        "watch:labextension": "jupyter labextension watch ."
    },
    "dependencies": {
        "@jupyter-widgets/base": "^6.0.6"
    },
    "devDependencies": {
        "@babel/core": "^7.23.9",
        "@babel/preset-env": "^7.23.9",
        "@jupyter-widgets/base-manager": "^1.0.7",
        "@jupyterlab/builder": "^4.0.11",
        "@lumino/application": "^2.3.0",
        "@lumino/widgets": "^2.3.1",
        "@types/jest": "^29.5.11",
        "@types/webpack-env": "^1.18.4",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "acorn": "^8.11.3",
        "css-loader": "^6.9.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "file-loader": "^6.2.0",
        "fs-extra": "^11.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "less": "^4.2.0",
        "less-loader": "^12.1.0",
        "mkdirp": "^3.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.2.4",
        "rimraf": "^5.0.5",
        "source-map-loader": "^5.0.0",
        "style-loader": "^3.3.4",
        "ts-jest": "^29.1.2",
        "ts-loader": "^9.5.1",
        "typescript": "^5.3.3",
        "webpack": "^5.90.0",
        "webpack-cli": "^5.1.4"
    },
    "jupyterlab": {
        "extension": "lib/plugin",
        "outputDir": "ipypandas/labextension/",
        "sharedPackages": {
            "@jupyter-widgets/base": {
                "bundled": false,
                "singleton": true
            }
        }
    }
}
