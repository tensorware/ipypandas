{
    "name": "ipypandas",
    "version": "0.0.5",
    "description": "Interactive JupyterLab features for the python data analysis library pandas.",
    "keywords": [
        "widgets",
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/tensorware/ipypandas",
    "author": {
        "name": "Tensorware"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tensorware/ipypandas"
    },
    "bugs": {
        "url": "https://github.com/tensorware/ipypandas/issues"
    },
    "files": [
        "src/styles/*.css",
        "dist/*.js",
        "lib/*.js"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "style": "src/styles/index.css",
    "jupyterlab": {
        "extension": "lib/plugin",
        "outputDir": "ipypandas/labextension",
        "sharedPackages": {
            "@jupyter-widgets/base": {
                "bundled": false,
                "singleton": true
            }
        }
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 200,
        "singleQuote": true,
        "trailingComma": "none",
        "endOfLine": "auto"
    },
    "jest": {
        "rootDir": "src/tests",
        "preset": "ts-jest/presets/js-with-babel",
        "testEnvironment": "jest-environment-jsdom",
        "testRegex": "test_.*.ts[x]?$",
        "transform": {
            "^.+\\.(ts|tsx)$": [
                "ts-jest"
            ]
        },
        "transformIgnorePatterns": [
            "node_modules/(?!(@jupyter(lab|-widgets)/.*)/)"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy"
        },
        "setupFilesAfterEnv": [
            "./jest.setup.js"
        ]
    },
    "scripts": {
        "test": "jest && pytest",
        "lint": "eslint . --ext .ts,.tsx",
        "build": "jlpm run build:less && jlpm run build:lib && jlpm run build:nbextension && jlpm run build:labextension",
        "build:lib": "tsc",
        "build:less": "lessc src/styles/index.less src/styles/index.css",
        "build:nbextension": "webpack --mode=production",
        "build:labextension": "jupyter labextension build .",
        "clean": "jlpm run clean:lib && jlpm run clean:docs && jlpm run clean:nbextension && jlpm run clean:labextension",
        "clean:lib": "rimraf lib dist",
        "clean:docs": "rimraf docs/build",
        "clean:nbextension": "rimraf ipypandas/nbextension",
        "clean:labextension": "rimraf ipypandas/labextension",
        "watch": "run-p watch:lib watch:nbextension watch:labextension",
        "watch:lib": "tsc -w --sourceMap",
        "watch:nbextension": "webpack --watch --mode=development",
        "watch:labextension": "jupyter labextension watch ."
    },
    "dependencies": {
        "@jupyter-widgets/base": "^6.0.7"
    },
    "devDependencies": {
        "@babel/core": "^7.23.9",
        "@babel/preset-env": "^7.23.9",
        "@jupyter-widgets/base-manager": "^1.0.8",
        "@jupyterlab/builder": "^4.1.1",
        "@lumino/application": "^2.3.0",
        "@lumino/widgets": "^2.3.1",
        "@types/jest": "^29.5.12",
        "@types/webpack-env": "^1.18.4",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@typescript-eslint/parser": "^7.0.1",
        "acorn": "^8.11.3",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^6.10.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "file-loader": "^6.2.0",
        "fs-extra": "^11.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "less": "^4.2.0",
        "less-loader": "^12.2.0",
        "mkdirp": "^3.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "source-map-loader": "^5.0.0",
        "style-loader": "^3.3.4",
        "ts-jest": "^29.1.2",
        "ts-loader": "^9.5.1",
        "typescript": "^5.3.3",
        "webpack": "^5.90.2",
        "webpack-cli": "^5.1.4"
    }
}
